#include<stdio.h>
#include<stdlib.h>
#define N 10
void Map()
{
	char map[N][N];
	int i, j;
	for (i = 0; i < 10; i++)
	for (j = 0; j < 10; j++)
		map[i][j] = '#';
	for (int i = 0; i < 10; i++)
	{
		for (int j = 0; j < 10; j++)
			printf("%c,", map[i][j]);
		printf("\n");
	}
	system("pause");
}
void Map1()
{
	char map[N][N];
	int i, j;
	for (i = 0; i < 10; i++)
	for (j = 0; j < 10; j++)
		map[i][j] = '#';
	map[0][0] = '@';
	for (i = 0; i < 10; i++)
	{
		for (j = 0; j < 10; j++)
			printf("%c,", map[i][j]);
		printf("\n");
	}
	system("pause");
}
int main()
{
	Map();
	Map1();
	system("pause");
	return 0;
}
